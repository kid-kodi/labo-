{
  "name": "angular-phonecat",
  "private": true,
  "version": "0.0.0",
  "description": "A tutorial application for AngularJS",
  "repository": "https://github.com/angular/angular-phonecat",
  "license": "MIT",
  "dependencies": {
    "express": "3.2.x",
    "mongodb": "2.x",
    "socket.io": "1.x",
    "JSV": "4.0.x",
    "nodemon": "*",
    "xlsx": "*",
    "socketio-auth": "*",
    "jsonwebtoken": "*",
    "crypto": "*",
    "muter": "*",
    "connect-busboy": "*",
    "body-parser": "*",
    "angular": "1.7.x",
    "angular-route": "1.7.x",
    "angular-resource": "1.7.x",
    "angular-cookies": "1.7.x",
    "angular-animate": "1.7.x",
    "angular-moment": "1.x",
    "angular-i18n": "1.x",
    "moment": "^1.x",
    "html2canvas": "*",
    "pdfmake": "*",
    "jquery": "*",
    "jqueryui": "*",
    "bootstrap": "3.3.x"
  },
  "devDependencies": {
    "angular-mocks": "1.7.x",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "nodemon": "^1.18.9",
    "protractor": "^5.4.1"
  },
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,jquery,jqueryui,jquery-*,moment,html2canvas,pdfmake,bootstrap/dist}/**/*\" public/lib -C",
    "prestart": "npm install",
    "start": "http-server ./public -a localhost -p 8000 -c-1",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "npm test -- --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  }
}
